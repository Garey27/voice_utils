#
set autoscale
#set logscale x
set mxtics 5
set mytics 2
set grid xtics ytics
set grid mxtics mytics
show grid
plot [0:22050] 'A' using (22050*$1):2 title "response" with lines,\
              'A' using (22050*$1):3 title "error" with lines
pause -1 "Hit return to continue"
quit

